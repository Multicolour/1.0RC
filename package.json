{
  "name": "multicolour",
  "version": "0.6.16",
  "description": "Generate your REST API with JSON and use almost any SQL DB tech.",
  "keywords": [
    "Multicolour",
    "REST API",
    "generator",
    "ORM",
    "CQRS",
    "microservice",
    "event-stream"
  ],
  "main": "index.js",
  "preferGlobal": true,
  "bin": {
    "multicolour": "cli.js"
  },
  "scripts": {
    "linters": "npm run flow && npm run eslint",
    "eslint": "eslint ./**/*.js",
    "flow": "flow",
    "unit-tests": "jest tests/*.js --coverage",
    "test": "npm run linters && npm run unit-tests",
    "changelog": "github_changelog_generator -u Multicolour -p multicolour"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Multicolour/multicolour.git"
  },
  "author": "newworldcode <hello@newworld.codes> (https://newworld.codes)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Multicolour/multicolour/issues"
  },
  "homepage": "https://github.com/Multicolour/multicolour#readme",
  "dependencies": {
    "ajv": "^6.5.2",
    "bluebird": "^3.5.1",
    "flow-typed": "^2.5.1",
    "knex": "^0.15.2"
  },
  "engine-strict": true,
  "engines": {
    "node": ">= 8.11.3"
  },
  "devDependencies": {
    "flow-bin": "^0.77.0",
    "jest": "^23.4.2",
    "jest-flow-transform": "^1.0.1",
    "newman": "^3.10.0",
    "prettier": "1.14.0"
  },
  "jest": {
    "transform": {
      "^.+\\.js(?:\\.flow)?$": "jest-flow-transform"
    }
  }
}
